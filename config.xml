<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.RTLFG" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Realm LFG</name>
    <description>
        Final Mobile app project LFG team finder.
    </description>
    <author email="Jweadick8689@conestogac.on.ca" href="http://Conestoga.ca">
        Jordan G. Weadick
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
<icon  src="Jw-icon.png"></icon>
<splash src="realmbanner.png"></splash>
</widget>
